@model TiendaDeportiva.ViewModels.ListUsersViewModel

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <ul>
        @foreach (var item in Model.Users)
        {
            <li>
                <label>Nombre : @Html.DisplayFor(modelItem => item.Nombre)</label>
                <label>Apellido: @Html.DisplayFor(modelItem => item.Apellido)</label>
                <label>Edad: @Html.DisplayFor(modelItem => item.Edad)</label>
                <a asp-controller="Store" asp-action="EditUser" asp-route-id="@item.Id">Editar</a>        
            </li>            
        }
    </ul>
</body>
</html>