@model TiendaDeportiva.ViewModels.ListUsersViewModel

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <ul>
        @foreach (var item in Model.Users)
        {
            <li>Nombre : @Html.DisplayFor(modelItem => item.Nombre)</li>
            <li>Apellido: @Html.DisplayFor(modelItem => item.Apellido)</li>
            <li>Edad: @Html.DisplayFor(modelItem => item.Edad)</li>
        }
    </ul>
</body>
</html>